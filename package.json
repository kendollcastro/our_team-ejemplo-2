{
  "name": "laboratorio_2",
  "version": "1.0.0",
  "description": "agregar estilos a las secciones de la pagina",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass -w sass -o public/css",
    "sass-build": "node-sass sass -o public/css --output-style compressed",
    "dev-server": "live-server --watch=\"index.html,sass,js\"",
    "dev": "npm run sass-build && concurrently \"npm run sass\" \"npm run dev-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/James-Gamboa/laboratorio_2.git"
  },
  "author": "Valeria Murillo, Jason Uma√±a",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/James-Gamboa/laboratorio_2/issues"
  },
  "homepage": "https://github.com/James-Gamboa/laboratorio_2#readme",
  "dependencies": {
    "concurrently": "^7.5.0",
    "live-server": "^1.2.2",
    "livereload": "^0.9.3",
    "node-sass": "^7.0.3"
  }
}
